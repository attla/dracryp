{
    "name": "attla/pincryp",
    "description": "A layer of encryption a little too judicious.",
    "keywords": [
        "attla",
        "laravel",
        "cipher",
        "encrypt",
        "encrypter",
        "cripter"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Nicolau",
            "email": "nicolau@octha.com"
        }
    ],
    "require": {
        "php": ">=7.2.5",
        "attla/support": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "Attla\\Pincryp\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "aliases": {
                "Pincryp": "Attla\\Pincryp\\Facade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "phpbench/phpbench": "^1.2"
    },
    "autoload-dev": {
        "psr-4": {
            "Attla\\Benchmark\\": "tests/benchmark/"
        }
    },
    "scripts": {
        "benchmark": "./vendor/bin/phpbench run --report=default"
    }
}
